{
  "expo": {
    "name": "Wurth",
    "slug": "Wurth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.ico",
    "scheme": "wurth",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/wurth2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.wurth.wurth"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.ico",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wurth.wurth"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.ico"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
